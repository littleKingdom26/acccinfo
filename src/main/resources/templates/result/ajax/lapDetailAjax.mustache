            </td>
            <td class="text-center"><b>{{lapTime}}</b></td>
            <td class="text-center">{{sector1}}</td>
            <td class="text-center">{{sector2}}</td>
            <td class="text-center">{{sector3}}</td>
        </tr>
    {{/eventSubList}}
</table>
