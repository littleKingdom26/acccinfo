{{>admin/layout/header}}
<body>
<div id="wrap">
	<div class="inWrap">
		{{>admin/layout/lnb}}
		<div id="container">
			{{>admin/layout/topBar}}
			<div class="location">
				<em class="home">HOME</em><i></i>
				<em>팀관리</em><i></i>
				<em class="active">팀원 관리</em>
			</div>
			<div class="content">
				<div class="box">
					<div class="inBox">
						<form id="searchFrm" action="/admin/team/team" method="get">
							<table class="formTable">
								<colgroup>
									<col width="10%" />
									<col width="*" />
								</colgroup>
								<tbody>
								<tr>
									<th>팀 선택</th>
									<td>
										<select id="teamInfoSeq" name="teamInfoSeq">
											<option value="">선택</option>
											{{#teamInfoList}}
												<option value="{{teamInfoSeq}}"> {{teamName}}</option>
											{{/teamInfoList}}
										</select>
									</td>
								</tr>

								</tbody>
							</table>
							<div class="btnArea mt20">
								<a href="javascript:void(0)" id="btnSearch" class="btn btnBi btnBlue">조회</a>
							</div>
						</form>
					</div>
				</div>
				<div class="box">
					<div class="inBox">
						<table class="listTable mt10">
							<colgroup>
								<col width="50%" />
								<col width="20%" />
							</colgroup>
							<thead>
							<tr>
								<th>드라이버 명</th>
								<th>삭제</th>
							</tr>
							</thead>
							<tbody>

							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
		{{>admin/layout/bodyFooter}}
	</div>
</div>
</body>
{{>admin/layout/footer}}
<script src="/js/adminTeam.js"></script>
<script>
	$(function () {
		team.init();
	});

</script>