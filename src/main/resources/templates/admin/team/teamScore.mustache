{{>admin/layout/header}}
<body>
<div id="wrap">
	<div class="inWrap">
		{{>admin/layout/lnb}}
		<div id="container">
			{{>admin/layout/topBar}}
			<div class="location">
				<em class="home">HOME</em><i></i>
				<em>팀관리</em><i></i>
				<em class="active">팀 점수</em>
			</div>
			<div class="content">
				<div class="box">
					<div class="inBox">
						<form id="searchFrm" action="" method="get">
							<table class="formTable">
								<colgroup>
									<col width="10%" />
									<col width="*" />
									<col width="10%" />
								</colgroup>
								<tbody>
									<tr>
										<th>기간 선택</th>
										<td>
											<select id="searchEventDt" name="searchEventDt">
												<option value="">선택</option>
												{{#eventList}}
													<option value="{{eventDt}}"> {{eventDt}}</option>
												{{/eventList}}
											</select>
										</td>
										<td><a href="javascript:void(0)" id="btnSearch" class="btn btnBi btnBlue">조회</a></td>
									</tr>
								</tbody>
							</table>
						</form>
					</div>
				</div>
				<div class="box">
					<div class="inBox">
						<table class="listTable mt10">
							<colgroup>
								<col width="10%" />
								<col width="50%" />
								<col width="30%" />
							</colgroup>
							<thead>
							<tr>
								<th>순위</th>
								<th>팀명</th>
								<th>점수</th>
							</tr>
							</thead>
							<tbody id="teamScoreTable">
								<tr>
									<td colspan="3">팀을 선택하세요</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
		{{>admin/layout/bodyFooter}}
	</div>
</div>
<div id="divLayer" class="layer event_write-pop">

</div>
</body>
{{>admin/layout/footer}}
<script src="/js/adminTeam.js"></script>
<script>
	$(function () {
		teamScore.init();
	});

</script>